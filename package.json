{
  "name": "joint-hello-theme",
  "homepage": "https://rankreallyhigh.com/",
  "description": "A Joint theme for Elementor.",
  "version": "1.4.0",
  "scripts": {
    "update-version": "node .github/scripts/update-version-in-files.js",
    "build:prod": "grunt default --environment=production && webpack --env=development && webpack --env=production",
    "build:dev": "concurrently \"grunt default\" \"webpack --env=developmentLocal\" \"webpack --env=productionLocal\"",
    "build:dev:watch": "concurrently \"grunt default watch\" \"webpack --env=developmentLocalWithWatch\" \"webpack --env=productionLocalWithWatch\"",
    "clean:build": "rimraf build && rimraf hello-joint",
    "rename": "node -e \"require('fs').rename('build', 'hello-joint', function(err) { if (err) console.log(err); console.log('File rename.') })\"",
    "zip": "npm run clean:build && npm run build:prod && npm run rename && npm run ziponly",
    "ziponly": "dir-archiver --src hello-joint --dest hello-joint.$npm_package_version.zip",
    "lint:js": "eslint .",
    "demo": "echo $npm_package_version",
    "check-env": "node -e console.log(process.env.npm_package_version)"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-optional-chaining": "^7.17.12",
    "@babel/plugin-transform-modules-commonjs": "^7.18.2",
    "@babel/plugin-transform-runtime": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@wordpress/eslint-plugin": "^12.3.0",
    "autoprefixer": "^9.8.8",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.2.1",
    "copy-webpack-plugin": "^11.0.0",
    "cssnano": "^4.1.11",
    "dir-archiver": "^1.2.0",
    "eslint": "^8.16.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-no-jquery": "^2.7.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "grunt": "^1.5.3",
    "grunt-autoprefixer": "^3.0.4",
    "grunt-checktextdomain": "^1.0.1",
    "grunt-contrib-sass": "^2.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-cssnano": "^3.0.0",
    "grunt-postcss": "^0.9.0",
    "grunt-sass": "^3.1.0",
    "grunt-text-replace": "^0.4.0",
    "grunt-webpack": "^5.0.0",
    "grunt-wp-readme-to-markdown-with-extra": "^2.2.0",
    "load-grunt-tasks": "^5.1.0",
    "marked": "^4.0.16",
    "matchdep": "^2.0.0",
    "node-sass": "^7.0.1",
    "npm-build-zip": "^1.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  }
}
